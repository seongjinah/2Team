<!DOCTYPE html PUBLIC “-//W3C//DTD HTML 4.0 Transitional//EN”>
<html>
	<head>
		<script>
			var urllist = '{{ url_list }}';
			var url_ary=urllist.split(",");
			var wordnum = '{{ num }}';
			var num_ary=wordnum.split(",");
			var time = '{{ time }}';
			var time_ary=time.split(",");
			var size='{{ size }}';
			size = Number(size);
		</script>
		<title>2Team_OpenSource_TemaProject_textfile</title>
		<style>
			h1 { text-align: center; }
		</style>
	</head>
	<body>
		<h1>URL Analysis Service</h1>
		<h3>text file: {{ filename }} </h3>
		<br>
		<script>
			for(var i=0;i<size;i++){
				document.write(url_ary[i]);
				document.write("<br>");
				document.write(num_ary[i]);
				document.write("<br>");
				document.write(time_ary[i]);
				document.write("<br>");
				document.write("<br>");
			}
		</script>
	</body>
</html>
